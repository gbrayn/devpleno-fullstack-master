  <%- include ('header') %>
  <div class="container">
    <h2 id="open-roles">Open roles:</h2>
      <% categorias.forEach(categoria => { %>
        <h3><%- categoria.categoria %></h3>
        <ul class="list-group">
          <% categoria.vagas.forEach(vaga => {%>
            <li class="list-group-item">
              <a href="/vaga/<%- vaga.id %>"><%- vaga.titulo %></a>
            </li>
          <% }) %>
        </ul>
      <%})%>
    </div>
  <%- include('footer') %>
</html>
